{
  "include": ["./node_modules/@types/jest", "cypress", "src"],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "types": ["@types/jest", "cypress"],
    "lib": ["dom", "esnext"],
    "jsx": "react",
    "target": "es5",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "baseUrl": "src"
  }
}
